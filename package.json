{
  "name": "dbs-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "deploy": "npm run write:env -s && ng deploy",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run write:env -s && ng e2e",
    "cypress:open": "npm run write:env -s && ng run dbs-front:cypress-open",
    "cypress:run": "npm run write:env -s && ng run dbs-front:cypress-run",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s && husky install",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --release --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "~14.0.0-next.10",
    "@angular/cdk": "~13.1.0",
    "@angular/common": "~14.0.0-next.10",
    "@angular/compiler": "~14.0.0-next.10",
    "@angular/core": "~14.0.0-next.10",
    "@angular/fire": "^7.2.1",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "https://dl.circleci.com/private/output/job/026a218d-c4d2-4bd9-8edd-43b5e199fdf7/artifacts/0/angular/forms-pr43834-17ce0ac4a6.tgz",
    "@angular/localize": "~14.0.0-next.10",
    "@angular/material": "~13.1.0",
    "@angular/material-experimental": "^14.0.0-next.9",
    "@angular/platform-browser": "~14.0.0-next.10",
    "@angular/platform-browser-dynamic": "~14.0.0-next.10",
    "@angular/router": "~14.0.0-next.10",
    "@angular/service-worker": "~14.0.0-next.10",
    "@awesome-cordova-plugins/core": "^5.30.0",
    "@awesome-cordova-plugins/keyboard": "^5.30.0",
    "@awesome-cordova-plugins/splash-screen": "^5.30.0",
    "@awesome-cordova-plugins/status-bar": "^5.30.0",
    "@ngx-translate/core": "^14.0.0",
    "cordova-android": "^10.1.1",
    "cordova-custom-config": "^5.1.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^3.0.0",
    "date-fns": "^2.16.1",
    "firebase": "^9.4.0",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^6.1.0",
    "rxfire": "^6.0.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0-next.7",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/schematics": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~14.0.0-next.7",
    "@angular/compiler-cli": "~14.0.0-next.10",
    "@angular/language-service": "~14.0.0-next.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@cypress/schematic": "^1.5.0",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-rocket/scripts": "^5.2.1",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.20.37",
    "@typescript-eslint/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "~5.6.0",
    "cordova": "^10.0.0",
    "cypress": "~9.1.1",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "hads": "^3.0.0",
    "htmlhint": "^1.0.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^7.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "~14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~5.0.2",
    "stylelint-config-standard": "~24.0.0",
    "ts-node": "^10.1.0",
    "typescript": "~4.6.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-custom-config": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  }
}
